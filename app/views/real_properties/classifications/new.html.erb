<%= simple_form_for [@real_property, @real_property_classification], url: :real_property_classifications do |f| %>
  <%= f.input :classification_id, collection: Configurations::Classification.all.map {|a| [a.name, a.id] } %>
  <%= f.input :effectivity_date, input_html: { input_html: "datepicker" } %>
  <%= f.submit "Update Classification" %>
<% end %>
