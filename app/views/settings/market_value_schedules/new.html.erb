<%= simple_form_for [@sub_classification, @market_value], url: :settings_sub_classification_market_value_schedules do |f| %>
  <%= f.input :market_value %>
  <%= f.input :unit %>
  <%= f.input :effectivity_date, input_html: { class: "datepicker" } %>
  <%= f.input :municipality_id, collection: Locations::Municipality.all.map{|a| [a.name, a.id] } %>
  <%= f.submit "Create Market Value" %>
<% end %>
